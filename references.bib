language = {eng},
month = {feb},
number = {2},
pages = {308--317},
pmid = {20888417},
title = {{17A, a novel non-coding RNA, regulates GABA B alternative splicing and signaling  in response to inflammatory stimuli and in Alzheimer disease.}},
